{
  "manifest_version": 2,
  "name": "Youtube mp3 downloander",
  "description": "Youtube mp3 downloander",
  "version": "1.0",
   "background": {
    "scripts":["background.js","jquery-3.2.1.js"]
  },
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png"},
  "browser_action": {
          "default_icon": "icon48.png",
          "default_title": "Youtube block videos"            
        } ,
      "content_scripts": [
    { "run_at":"document_end",
      "matches": ["*://www.onlinevideoconverter.com/*"],
      "js": ["jquery-3.2.1.js", "contentscript.js"]
    }
  ],
  "permissions": [
          "*//www.onlinevideoconverter.com/*","tabs"
        ]
          }